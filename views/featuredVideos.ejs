<% blogs.forEach(function(blog) { %>
  <div class="blogEntry">
    <div class="imgWrapper playSong" data-name="<%=blog.name%>" data-artist="<%=blog.artist%>" data-vid="<%=blog.vid%>">
      <img class="blogImg" src="/images/<%=blog.vid%>.jpg" onerror="replaceImg(event)"/>
      <span class="glyphicon glyphicon-play"></span>
    </div>
    <div class="blogEntryBody">
      <div class="blogEntryName"><%= blog.name %></div>
      <div class="blogEntryArtist"><%= blog.artist %></div>
      <br>
      <div class="blogEntryText"><%= blog.text %></div>
    </div>
  </div>
<% }); %>
