<div id="vidPlayer">
    <video autoplay loop muted>
      <source src="images/muramasa1.mp4" type="video/mp4">
    </video>
  <div id="vidPlayerCover"></div>
</div>
<div id="finder">
<div id="findVideos">E L E V A T E</div>
<input style="text" id="findVideosText" placeholder="Search Song, Artist, Director, etc."/>
<% if(spotify) { %>
  <div id="spotifyLogin">Change Spotify</div>
<% } else { %>
  <div id="spotifyLogin">Sync Spotify</div>
<% } %>
<div id="findVideosOr">- OR BROWSE -</div>
<div id="searchCriteria">
        <div id="genreDrop" class="criteriaType">
        <div class="vtitle">GENRE:</div>
          <div id="vDance" class="vItem" data-search="dance">
            <div class="itemName">Dance</div>
          </div>
          <div id="vDubstep" class="vItem searched" data-search="dubstep">
            <div class="itemName">Dubstep</div>
          </div>
          <div id="vElectronic" class="vItem" data-search="electronic">
            <div class="itemName">Electronic</div>
          </div>
          <div id="vExperimental" class="vItem" data-search="experimental">
            <div class="itemName">Experimental</div>
          </div>
          <div id="vChill" class="vItem" data-search="chill">
            <div class="itemName">Chill</div>
          </div>
          <div id="vRap" class="vItem" data-search="rap">
            <div class="itemName">Hip-Hop/Rap</div>
          </div>
          <div id="vComedy" class="vItem" data-search="comedy">
            <div class="itemName">Comedy</div>
          </div>
          <div id="vAlternative" class="vItem" data-search="alternative">
            <div class="itemName">Alternative</div>
          </div>
        </div>
        <br>
  <div id="sortDrop" class="criteriaType">
          <div class="vtitle">SORT BY:</div>
          <div id="vitemRelevant" class="vItem" data-search="relevance">
            <div class="itemName">Relevance</div>
          </div>
          <div id="vitemPopular" class="vItem searched" data-search="likes">
            <div class="itemName">Popularity</div>
          </div>
          <div id="vitemNewest" class="vItem" data-search="created_at">
            <div class="itemName">Newest</div>
          </div>
        </div>
        <br>
        <div id="filterDrop" class="criteriaType">
          <div class="vtitle">FILTER:</div>
          <div id="vitemStaff" class="vItem" data-search="staff">
            <div class="itemName">Staff Picks</div>
          </div>
          <% if(spotify) { %>
            <div id="vitemSpotifyArtists" class="vItem spotify" data-search="spotifyArtists">
              <div class="itemName">Spotify Artists</div>
            </div>
          <% } %>
        </div>
        <br>
        <div id="searchDrop" class="criteriaType">
          <div class="vtitle">SEARCH:</div>
          <div id="vitemSongs" class="vItem searched" data-search="name">
            <div class="itemName">Songs</div>
          </div>
          <div id="vitemArtists" class="vItem searched" data-search="artist">
            <div class="itemName">Artists</div>
          </div>
          <div id="vitemTags" class="vItem" data-search="tags">
            <div class="itemName">Playlists</div>
          </div>
        </div>
        <br>
        <div id="searchVideos">Search Videos</div>
</div>
</div>
