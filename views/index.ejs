    <% include navbar %>
    <div id="spotifyControls">
      <div class="spotifyTitle">Spotify</div>
      <% include spotifyLists %>
    </div>
    <div id="content">
      <% include home %>
    </div>
    <div id="listDetails" class="hidden">
      <div id="listTitle"></div>
      <div id="overlay"></div>
      <div id="cover" class="play"></div>
      <div id="prevPlayer"></div>
      <div id="songList">
        <div id="songSelector"></div>
        <div id='buffer'></div>
      </div>
    </div>
    <div id="player" data-vid="-" data-id="-" data-vidno="-" data-lid=""><div id="videoPlayer"></div>
      <div id="controls" class="home">
        <div id="fullScreenWrapper">
          <span class="glyphicon glyphicon-resize-full"></span>
        </div>
        <div id="playerExpand">
          <div class="controls-menu-up"></div>
          <div id="playingName"></div>
        </div>
        <div id="playToggleSelection"></div>
        <div id="playToggle">
          <div id="playTogglePauseLeft"></div>
          <div id="playTogglePauseRight"></div>
          <span class="glyphicon glyphicon-play"></span>
        </div>
      </div>
      <div id="playercover" class="playerOptions">
        <div id="songPlaylist">Show Playlist</div>
        <div id="songTitle"></div>
        <div id="songArtist" data-artist-id=''></div>
        <div id="relatedSongs" class="playerOptions" onclick="findRelatedSongs()">Related Songs</div>
        <div id="shuffle"><span class="glyphicon glyphicon-random"></span></div>
        <div id="repeat" class="active"><span class="glyphicon glyphicon-repeat"></span></div>
      </div>
      <div id="playerCollapse" class="playerOptions"><span class="glyphicon glyphicon-menu-down"></span></div>
      <!-- <div id="likeSong" class="playerOptions" onclick="playerLikeSong(event)"><span class="glyphicon glyphicon-heart-empty"></span></div> -->
      <div id="nextSong" class="playerOptions"><span class="glyphicon glyphicon-menu-right"></span></div>
      <div id="previousSong" class="playerOptions"><span class="glyphicon glyphicon-menu-left"></span></div>

    </div>
